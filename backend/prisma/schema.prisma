datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String
  password  String
  role      String   @default("member")
  createdAt DateTime @default(now())
}

model Class {
  id        String   @id @default(cuid())
  title     String
  capacity  Int
  createdAt DateTime @default(now())
}
